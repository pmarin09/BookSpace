<p><% @coworkingspaces.each do |space| %> </p>
<div class="card-deck">
  <div class="card">
    <%= link_to (image_tag space.pic, class: "card-img-top"), space %>
    <div class="card-body">
      <h5 class="card-title"><%= space.name %></h5>
      <p class="card-text"><%= space.address %></p>
      <p class="card-text"><%= space.spaces_qty %> spaces available</p>
      <%= button_to "Book Now", "/coworkingspaces/#{space.id}", :method => "get", class: "btn btn-primary" %>
    </div>
    <div class="card-footer">
      <small class="text-muted">Last updated 3 mins ago</small>
    </div>
  </div>
</div>
<%end%>
